    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3",
    "react-router-hash-link": "^2.4.3",
    "recoil": "^0.7.7",
